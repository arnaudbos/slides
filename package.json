{
  "name": "slides",
  "version": "0.0.0",
  "author": "Igor Laborie <ilaborie@gmail.com>",
  "license": "MIT",
  "scripts": {
    "serve": "lite-server",
    "serve:ex": "ts-node src/main/typescript/server.ts",
    "style:slides": "node-sass src/main/style/slides.scss --output src/main/web/",
    "style:print": "node-sass src/main/style/print.scss --output src/main/web/",
    "style:cssIsAwesome": "node-sass src/test/style/cssIsAwesome.scss --output src/main/web/",
    "style:koltinBbl": "node-sass src/test/style/kotlinBbl.scss --output src/main/web/",
    "style:koltinHandsOn": "node-sass src/test/style/kotlinHandsOn.scss --output src/main/web/",
    "style:bbl": "node-sass src/test/style/bbl.scss --output src/main/web/",
    "style:devfest-tls": "node-sass src/test/style/devfest-tls.scss --output src/main/web/",
    "style:devoxx-ma": "node-sass src/test/style/devoxx-ma.scss --output src/main/web/",
    "start": "run-p -nl serve \"style:** -- --watch\"",
    "publish": "gh-pages --dist src/main/web"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/highlight.js": "^9.1.10",
    "@types/marked": "^0.3.0",
    "@types/node": "^8.0.25",
    "@types/puppeteer": "^0.13.0",
    "@types/request": "^2.0.8",
    "@types/request-promise-native": "^1.0.10",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "lite-server": "^2.3.0",
    "node-sass": "~4.5.3",
    "npm-run-all": "^4.1.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "express": "^4.16.2",
    "highlight.js": "^9.12.0",
    "marked": "^0.3.6",
    "puppeteer": "^0.13.0",
    "request": "^2.34",
    "request-promise-native": "^1.0.4"
  }
}
