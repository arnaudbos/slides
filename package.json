{
  "name": "slides",
  "version": "0.0.0",
  "author": "Igor Laborie <ilaborie@gmail.com>",
  "license": "MIT",
  "scripts": {
    "serve": "lite-server",
    "style:slides": "node-sass src/main/style/slides.scss --output src/main/web/",
    "style:print": "node-sass src/main/style/print.scss --output src/main/web/",
    "style:cssIsAwesome": "node-sass src/test/style/cssIsAwesome.scss --output src/main/web/",
    "style:devfest-tls": "node-sass src/test/style/devfest-tls.scss --output src/main/web/",
    "style:watch": "npm run style:slides -- --watch & npm run style:print -- --watch & npm run style:cssIsAwesome -- --watch & npm run style:devfest-tls -- --watch",
    "start": "npm run serve & npm run style:watch",
    "publish": "gh-pages --dist src/main/web"
  },
  "devDependencies": {
    "@types/highlight.js": "^9.1.10",
    "@types/marked": "^0.3.0",
    "@types/node": "^8.0.25",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "lite-server": "^2.3.0",
    "node-sass": "^4.5.3",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "marked": "^0.3.6",
    "puppeteer": "^0.11.0",
    "request": "^2.34",
    "request-promise-native": "^1.0.4"
  }
}
